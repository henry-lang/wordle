{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"plugins": [{ "name": "typescript-plugin-css-modules" }],
		"target": "es6",
		"lib": ["es5", "es6", "dom"],
		"jsx": "react",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"moduleResolution": "node"
	}
}